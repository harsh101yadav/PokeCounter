    "import requests\n",
    "\n",
    "\n",
    "url = f\"https://pokeapi.co/api/v2/pokemon\"\n",
    "response = requests.get(url)\n",
    "if response.status_code == 200:\n",
    "    pokemon_data = response.json()\n",
    "    print(pokemon_data)\n",
    "else:\n",
    "    print(\"Failed to fetch data\")"
